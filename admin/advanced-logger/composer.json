{
    "name": "savannabits/advanced-logger",
    "description": "Advanced logger",
    "keywords": [
        "logger",
        "request logger"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Samson Maosa",
            "email": "smaosa@strathmore.edu",
            "homepage": "https://www.savannabits.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0",
        "monolog/monolog": "^1.12|^2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "laravel/framework": "6.*|7.*",
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "~4.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Savannabits\\AdvancedLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Savannabits\\AdvancedLogger\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Savannabits\\AdvancedLogger\\AdvancedLoggerServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
